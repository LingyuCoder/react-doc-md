{
  "name": "react-prop-table",
  "version": "0.1.1",
  "description": "Parse react source file and generate html or markdown table",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/_mocha --require should --reporter spec",
    "test-cov": "NODE_ENV=test node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --require should",
    "test-travis": "NODE_ENV=test node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- --require should"
  },
  "author": "LingyuCoder",
  "license": "MIT",
  "devDependencies": {
    "istanbul": "~0.4.0",
    "markdown-tableify": "0.0.4",
    "mocha": "~2.3.3",
    "should": "~7.1.1",
    "uncolor": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ly-tools/react-prop-table"
  },
  "keywords": [
    "react",
    "proptype",
    "table",
    "markdown",
    "html",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/ly-tools/react-prop-table/issues"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "dependencies": {
    "cli-table2": "^0.1.9",
    "html-tableify": "0.0.2",
    "humanize-proptype": "0.0.3",
    "lodash": "~4.17.19",
    "markdown-tableify": "0.0.4",
    "react-docgen": "~2.4.0"
  }
}
